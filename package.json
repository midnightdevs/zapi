{
  "name": "zAPI",
  "version": "0.0.1",
  "description": "Model API with FastFy",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack",
    "serve:dev": "cross-env NODE_ENV=development supervisor build/index.js",
    "start:dev": "cross-env NODE_ENV=development webpack",
    "start": "node build/index.js",
    "lint": "eslint --ext js,jsx src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": ["lint-staged"]
    }
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcelFox/zAPI.git"
  },
  "keywords": [
    "fastfy",
    "nodejs",
    "javascript"
  ],
  "author": "Marcel Fox",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MarcelFox/zAPI/issues"
  },
  "homepage": "https://github.com/MarcelFox/zAPI#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "fastify": "^3.22.0",
    "fastify-express": "^0.3.3",
    "fastify-webpack-hmr": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "path": "^0.12.7",
    "supervisor": "^0.12.0",
    "webpack": "^5.55.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.3.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "rimraf": "^3.0.2",
    "webpack-cli": "^4.8.0"
  }
}
