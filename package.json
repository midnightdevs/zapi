{
  "name": "dasmodell",
  "version": "0.0.1",
  "description": "Model API with FastFy",
  "main": "main.js",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack",
    "serve:dev": "cross-env NODE_ENV=development supervisor build/bundle.js",
    "start:dev": "cross-env NODE_ENV=development webpack",
    "start": "node build/bundle.js",
    "lint": "eslint --ext .js,.ts ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcelFox/DasModell.git"
  },
  "keywords": [
    "fastfy",
    "nodejs",
    "javascript"
  ],
  "author": "Marcel Fox",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MarcelFox/DasModell/issues"
  },
  "homepage": "https://github.com/MarcelFox/DasModell#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.0",
    "fastify-express": "^0.3.3",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "supervisor": "^0.12.0",
    "webpack": "^5.55.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint-plugin-json-format": "^2.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack-cli": "^4.8.0"
  }
